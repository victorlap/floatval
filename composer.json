{
    "name": "victorlap/floatval",
    "type": "library",
    "description": "Correctly parse float like strings to float values",
    "keywords": [
        "victorlap",
        "floatval"
    ],
    "homepage": "https://github.com/victorlap/floatval",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Lap",
            "email": "victorlap@outlook.com",
            "homepage": "https://github.com/victorlap",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Victorlap\\Floatval\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victorlap\\Floatval\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
